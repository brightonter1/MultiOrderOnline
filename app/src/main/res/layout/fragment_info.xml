<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Store.infoFragment">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:id="@+id/info_phonenumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        android:textSize="20dp"
        android:fontFamily="@font/font_regular"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/info_open-close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="20dp"
        android:fontFamily="@font/font_regular"
        app:layout_constraintStart_toStartOf="@+id/info_phonenumber"
        app:layout_constraintTop_toBottomOf="@+id/info_phonenumber" />

    <TextView
        android:id="@+id/info_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="20dp"
        android:fontFamily="@font/font_regular"
        app:layout_constraintStart_toStartOf="@+id/info_open-close"
        app:layout_constraintTop_toBottomOf="@+id/info_open-close" />

</android.support.constraint.ConstraintLayout>